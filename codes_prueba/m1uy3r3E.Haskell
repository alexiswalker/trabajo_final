processMessages :: KafkaConsumer -> IO (Either KafkaError ()) processMessages kafka = do     mapM_ (\_ -> do                    msg1 <- pollMessage kafka (Timeout 1000)                    putStrLn $ "Message: " ++ show msg1                     err <- commitAllOffsets OffsetCommit kafka                    putStrLn $ "Offsets: " ++ maybe "Committed." show err           ) [1 :: Integer .. 4]     return $ Right ()