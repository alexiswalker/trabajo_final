      <rdb-sync ds-jndi="java:/datasources/saj_adv" update-interval="60" dispatcher="advCollectionGlobalDispatcherEjb">
         <record>
            <query>
select
arq.cdarquivo as id,
arq.flativo as flativo,
arq.flsigiloso as flsigiloso,
arqtip.cdarquivotipo as cdarquivotipo,
arqtip.nmarquivotipo as nmarquivotipo,
