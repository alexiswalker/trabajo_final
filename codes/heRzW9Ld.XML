   <collection name="atendimento">
      <field name="id" type="string" unique="true" />
      <field name="deIdentificador" type="string" />
      <field name="cdArrendatario" type="long" />
      <field name="flAtivo" type="string" />
      <field name="flHabilitado" type="string" />
      <field name="dtAtendimento" type="string" />
      <field name="nmSituacao" type="sp_phonetic" />
      <field name="nmAssunto" type="sp_phonetic" />
      <field name="deObservacao" type="sp_phonetic" />
      <field name="nmPasta" type="string" />
      <field name="nomeResponsavel" type="sp_phonetic" multi-valued="true" />
      <field name="nmGrupo" type="sp_phonetic" multi-valued="true" />
      <field name="nomeCliente" type="sp_phonetic" multi-valued="true" />
      <field name="nmMarcador" type="string" multi-valued="true" />
      <field name="text" type="text_general" stored="false" multi-valued="true" copy-from="deIdentificador,nmAssunto,nmPasta,nomeCliente,deObservacao,nmSituacao,nmMarcador" />
      <rdb-sync ds-jndi="java:/datasources/saj_adv" update-interval="60" dispatcher="advCollectionGlobalDispatcherEjb">
         <record>
            <query>
select
atend.cdatendimento as id,
atend.deidentificador as deidentificador,
atend.cdarrendatario as cdarrendatario,
atend.dtatendimento as dtatendimento,
atend.nmpasta as nmpasta,
atend.nmassunto as nmassunto,
atend.deobservacao as deobservacao,
atend.flativo as flativo,
atend.flhabilitado as flhabilitado,
ass.nmsituacao as nmsituacao
from atendimento atend
inner join atendimentosituacao ass on atend.cdatendimentosituacao = ass.cdatendimentosituacao
</query>
            <metadata>
               <table-pks name="atendimento" columns="cdatendimento" />
               <table-pks name="atendimentosituacao" columns="cdatendimentosituacao" />
               <column-type alias="id" java-type="BigDecimal" />
            </metadata>
            <logical-exclusions>
               <exclusion-clause table-name="atendimento">
                  <column-exclusion column="flativo" value="N" />
                  <column-exclusion column="flhabilitado" value="N" />
               </exclusion-clause>
               <exclusion-clause table-name="atendimentosituacao">
                  <column-exclusion column="flativo" value="N" />
               </exclusion-clause>
            </logical-exclusions>
         </record>
         <record name="grupo">
            <query>
select
atend.cdatendimento as id,
gr.nmgrupo as nmgrupo
from atendimento atend
inner join reatendimentogrupo rag on atend.cdatendimento = rag.cdatendimento
inner join grupo gr on rag.cdgrupo = gr.cdgrupo
</query>
            <metadata>
               <table-pks name="atendimento" columns="cdatendimento" />
               <table-pks name="reatendimentogrupo" columns="cdgrupo" />
               <table-pks name="grupo" columns="cdgrupo" />
            </metadata>
            <logical-exclusions>
               <exclusion-clause table-name="grupo">
                  <column-exclusion column="flativo" value="N" />
                  <column-exclusion column="flhabilitado" value="N" />
               </exclusion-clause>
            </logical-exclusions>
         </record>
         <record name="pessoa">
            <query>
select
atend.cdatendimento as id,
pr.nmpessoa as nomeresponsavel
from atendimento atend
inner join reatendimentousuario rau on atend.cdatendimento = rau.cdatendimento
inner join usuario ur on rau.cdusuario = ur.cdusuario
inner join pessoa pr on ur.cdpessoa = pr.cdpessoa
</query>
            <metadata>
               <table-pks name="atendimento" columns="cdatendimento" />
               <table-pks name="reatendimentousuario" columns="cdusuario" />
               <table-pks name="usuario" columns="cdpessoa" />
               <table-pks name="pessoa" columns="cdpessoa" />
            </metadata>
            <logical-exclusions>
               <exclusion-clause table-name="usuario">
                  <column-exclusion column="flativo" value="N" />
                  <column-exclusion column="flhabilitado" value="N" />
               </exclusion-clause>
               <exclusion-clause table-name="pessoa">
                  <column-exclusion column="flativo" value="N" />
                  <column-exclusion column="flhabilitado" value="N" />
               </exclusion-clause>
            </logical-exclusions>
         </record>
         <record name="atendimentopessoa">
            <query>
select
atend.cdatendimento as id,
pc.nmpessoa as nomecliente
from atendimento atend
inner join atendimentopessoa ap on atend.cdatendimento = ap.cdatendimento
inner join pessoa pc on ap.cdpessoa = pc.cdpessoa
</query>
            <metadata>
               <table-pks name="atendimento" columns="cdatendimento" />
               <table-pks name="atendimentopessoa" columns="cdpessoa" />
               <table-pks name="pessoa" columns="cdpessoa" />
            </metadata>
            <logical-exclusions>
               <exclusion-clause table-name="atendimentopessoa">
                  <column-exclusion column="flativo" value="N" />
               </exclusion-clause>
               <exclusion-clause table-name="pessoa">
                  <column-exclusion column="flativo" value="N" />
                  <column-exclusion column="flhabilitado" value="N" />
               </exclusion-clause>
            </logical-exclusions>
         </record>
         <record name="marcador">
            <query>
select
atend.cdatendimento as id,
marc.nmmarcador as nmmarcador
from atendimento atend
inner join marcadorvinculomodulo marcvinc on atend.cdatendimento = marcvinc.cdregistrovinculo and (marcvinc.cdmodulo = 1)
inner join marcador marc on marcvinc.cdmarcador = marc.cdmarcador
</query>
            <metadata>
               <table-pks name="atendimento" columns="cdatendimento" />
               <table-pks name="marcadorvinculomodulo" columns="cdregistrovinculo,cdmarcador" />
               <table-pks name="marcador" columns="cdmarcador" />
            </metadata>
            <logical-exclusions>
               <exclusion-clause table-name="marcador">
                  <column-exclusion column="flativo" value="N" />
               </exclusion-clause>
            </logical-exclusions>
         </record>
      </rdb-sync>
   </collection>
   <collection name="processo">
      <field name="id" type="string" unique="true" />
      <field name="deIdentificador" type="string" />
      <field name="cdArrendatario" type="long" />
      <field name="flAtivo" type="string" />
      <field name="flHabilitado" type="string" />
      <field name="nomePasta" type="sp_phonetic" />
      <field name="numeroProcesso" type="text_general" />
      <field name="nomeAssunto" type="sp_phonetic" />
      <field name="nomeSituacao" type="sp_phonetic" />
      <field name="nomeProcessoOrgaoExtraJudicial" type="sp_phonetic" />
      <field name="nomeProcessoOrgaoJudicial" type="sp_phonetic" />
      <field name="nomeUnidadeOrganizacional" type="sp_phonetic" />
      <field name="dataInclusao" type="string" />
      <field name="dataCitacao" type="string" />
      <field name="dataDistribuicao" type="string" />
      <field name="dataEncerramento" type="string" />
      <field name="dataJulgamento" type="string" />
      <field name="cdEstadoExtraJudicial" type="long" />
      <field name="cdEstadoJudicial" type="long" />
      <field name="cdAtendimento" type="long" />
      <field name="nomeEnvolvidos" type="sp_phonetic" multi-valued="true" />
      <field name="nmMarcador" type="string" multi-valued="true" />
      <field name="text" type="text_general" stored="false" multi-valued="true" copy-from="deIdentificador,nomePasta,numeroProcesso,nomeAssunto,nomeSituacao,nomeProcessoOrgaoExtraJudicial,nomeProcessoOrgaoJudicial,nomeUnidadeOrganizacional,nmMarcador" />
      <rdb-sync ds-jndi="java:/datasources/saj_adv" update-interval="60" dispatcher="advCollectionGlobalDispatcherEjb">
         <record>
            <query>
select
pro.cdprocesso as id,
pro.deidentificador as deidentificador,
pro.cdarrendatario as cdarrendatario,
pro.flativo as flativo,
pro.flhabilitado as flhabilitado,
pro.nmpasta as nomepasta,
pn.denumeroprocesso as numeroprocesso,
pro.nmassunto as nomeassunto,
ps.desituacao as nomesituacao,
poej.nmorgao as nomeprocessoorgaoextrajudicial,
poj.nmorgao as nomeprocessoorgaojudicial,
uo.nmunidadeorganizacional as nomeunidadeorganizacional,
pro.dtinclusao as datainclusao ,
pro.dtcitacao as datacitacao,
pro.dtdistribuicao as datadistribuicao,
pro.dtencerramento as dataencerramento,
pro.dtjulgamento as datajulgamento,
eej.cdestado as cdestadoextrajudicial,
ej.cdestado as cdestadojudicial,
ate.cdatendimento as cdatendimento
from processo pro
left join processosituacao ps on pro.cdsituacaoprocesso = ps.cdprocessosituacao
left join unidadeorganizacional uo on pro.cdunidadeorganizacionalatual = uo.cdunidadeorganizacional
left join processonumero pn on pn.cdprocesso = pro.cdprocesso and pn.flprincipal = 'S' and pn.flativo = 'S'
left join processoorgao poej on poej.cdprocessoorgao = pro.cdprocessoorgaoextrajudicial
left join cidade cej on cej.cdcidade = poej.cdcidade
left join estado eej on eej.cdestado = cej.cdestado
left join processoorgao poj on poj.cdprocessoorgao  = pro.cdprocessoorgaojudicial
left join cidade cj on cj.cdcidade = poj.cdcidade
left join estado ej on ej.cdestado = cj.cdestado
left join atendimento ate on pro.cdatendimento = ate.cdatendimento
</query>
            <metadata>
               <table-pks name="processo" columns="cdprocesso" />
               <table-pks name="processosituacao" columns="cdprocessosituacao" />
               <table-pks name="unidadeorganizacional" columns="cdunidadeorganizacional" />
               <table-pks name="processonumero" columns="cdprocessonumero" />
               <table-pks name="processoorgao" columns="cdprocessoorgao" />
               <table-pks name="cidade" columns="cdcidade" />
               <table-pks name="estado" columns="cdestado" />
               <table-pks name="atendimento" columns="cdatendimento" />
               <column-type alias="id" java-type="BigDecimal" />
            </metadata>
            <logical-exclusions>
               <exclusion-clause table-name="processo">
                  <column-exclusion column="flativo" value="N" />
                  <column-exclusion column="flhabilitado" value="N" />
               </exclusion-clause>
            </logical-exclusions>
         </record>
         <record name="processoenvolvido">
            <query>
select
pro.cdprocesso as id,
pep.nmpessoa as nomeenvolvidos
from processo pro
inner join processoenvolvido pe on pe.cdprocesso = pro.cdprocesso
inner join pessoa pep on pep.cdpessoa = pe.cdpessoa
</query>
            <metadata>
               <table-pks name="processo" columns="cdprocesso" />
               <table-pks name="processoenvolvido" columns="cdpessoa" />
               <table-pks name="pessoa" columns="cdpessoa" />
            </metadata>
            <logical-exclusions>
               <exclusion-clause table-name="processo">
                  <column-exclusion column="flativo" value="N" />
                  <column-exclusion column="flhabilitado" value="N" />
               </exclusion-clause>
               <exclusion-clause table-name="pessoa">
                  <column-exclusion column="flativo" value="N" />
                  <column-exclusion column="flhabilitado" value="N" />
               </exclusion-clause>
               <exclusion-clause table-name="processoenvolvido">
                  <column-exclusion column="flativo" value="N" />
               </exclusion-clause>
            </logical-exclusions>
         </record>
         <record name="marcador">
            <query>
select
pro.cdprocesso as id,
marc.nmmarcador as nmmarcador
from processo pro
inner join marcadorvinculomodulo marcvinc on pro.cdprocesso = marcvinc.cdregistrovinculo and (marcvinc.cdmodulo = 3)
inner join marcador marc on marcvinc.cdmarcador = marc.cdmarcador
</query>
            <metadata>
               <table-pks name="processo" columns="cdprocesso" />
               <table-pks name="marcadorvinculomodulo" columns="cdregistrovinculo,cdmarcador" />
               <table-pks name="marcador" columns="cdmarcador" />
            </metadata>
            <logical-exclusions>
               <exclusion-clause table-name="marcador">
                  <column-exclusion column="flativo" value="N" />
               </exclusion-clause>
            </logical-exclusions>
         </record>
      </rdb-sync>
   </collection>
