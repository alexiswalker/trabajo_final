using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Gyurika
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Kérlek adj meg egy számot");

            int y = int.Parse(Console.ReadLine());
            
            int z = y / 2;

           // int j = 0;
            if (z * 2 == y)
            {
                Console.WriteLine("Páros");

            }
            else
            {
                Console.WriteLine("Páratlan");
            }
            int u = 0;
            for (int i = 2; i < y - 1; i++)
            {
                if ( (y%i)==0)
                {
                    u = (u + 1);
                    ;
                    Console.WriteLine(u);
                }
                else
                {
                   ;
                    Console.WriteLine(u);
                }

            }

            if (u == 0)
            {
                Console.WriteLine("Prím");
            }
            else
            {
                Console.WriteLine("Nem prím");
            }
            int zs = u + 2;
            Console.Write("Összesen {1} és {0}", u + 2,z);
            Console.Write(" osztója van "+u+" kaka");
            string text = "";
            for (int i = 0; i < 5; i++)
            {
                text += i;
                i = i + 1;
                i += 1;
                i++;
                ++i;
            }
            Console.WriteLine(text);
            Console.ReadLine();
       
        }
    }
}