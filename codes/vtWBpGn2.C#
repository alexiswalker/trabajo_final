using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
 
namespace _14_var_areas
{
public class Program
{
    public static void Main()
    {
        int n = int.Parse(Console.ReadLine());
        if ((n >= 100 && n <= 200) || (n == 0))
        {
            Console.WriteLine("");
        }
        else
        {
            Console.WriteLine("invalid");
        }
    }
}